{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"lib": ["ES2020"],
		"baseUrl": ".",
		"rootDir": ".",
		"outDir": "out",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true
	},
	"exclude": [
		"node_modules"
	]
}
